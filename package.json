{
  "name": "tailwinder",
  "version": "1.0.0",
  "description": "collection of components for tailwind",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=development && tailwindcss -i ./src/base.css -o ./dist/main.css --watch",
    "build": "NODE_ENV=production && tailwindcss -i ./src/base.css -o ./dist/main.css --minify",
    "alpinjs-dev": "npx esbuild index.js --outfile=dist/bundle.js --bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miladimos/tailwinder.git"
  },
  "style": "dist/tailwinder.css",
  "keywords": [
    "tailwindcss",
    "tailwindcss framework",
    "html",
    "css",
    "javascript",
    "tailwind",
    "tailwind components",
    "tailwind elements",
    "tailwind library",
    "tailwind sections",
    "tailwind css",
    "tailwind ui",
    "tailwind css react",
    "tailwind css vue",
    "tailwind css angular"
  ],
  "author": "miladimos <miladimos@outlook.com>",
  "contributors": [

  ],
  "bugs": {
    "url": "https://github.com/miladimos/tailwinder/issues"
  },
  "homepage": "https://github.com/miladimos/tailwinder#readme",
  "devDependencies": {
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "alpinejs": "^3.10.2",
    "autoprefixer": "^10.4.5",
    "esbuild": "^0.14.38",
    "postcss": "^8.4.12"
  },
  "license": "MIT"
}
